<?xml version="1.0"?>
<launch>
	<!-- start control node -->
	<node pkg="bluerov_ros" type="depth_controller.py" name="depth_controller" output="screen"/>
	<node pkg="bluerov_ros" type="velocity_controller.py" name="velocity_controller" output="screen"/>
	<node pkg="bluerov_ros" type="heading_controller.py" name="heading_controller" output="screen"/>
	<node pkg="bluerov_ros" type="gamepad.py" name="gamepad" output="screen"/>
